# Next.js environment variables
# Copy this file to .env.local and add your API keys

# Google Gemini API key
GOOGLE_API_KEY=your_google_api_key_here

# OpenRouter API key for accessing various LLMs
OPENROUTER_API_KEY=your_openrouter_api_key_here

# API Security
API_SECRET_KEY=your_secure_api_key_here
NODE_ENV=development  # Change to 'production' in production environment
